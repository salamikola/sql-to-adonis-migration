{
  "name": "@skolawole/adonis-sql-to-migration",
  "version": "1.0.0",
  "description": "This package helps create  database migration from  existing database schema",
  "main": "index.js",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "lint": "eslint . --ext .ts",
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:files",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "release": "np --no-cleanup --no-tests",
    "commit": "git-cz"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/salamikola/sql-to-adonis-migration.git"
  },
  "keywords": [
    "Adonis",
    "5",
    "migration",
    "reverse migration",
    "sql",
    "sql to migration"
  ],
  "author": "Salami Kola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/salamikola/sql-to-adonis-migration/issues"
  },
  "homepage": "https://github.com/salamikola/sql-to-adonis-migration#readme",
  "peerDependencies": {
    "@adonisjs/config": "^3.0.9",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/lucid": "^18.3.0",
    "@adonisjs/repl": "^3.1.11"
  },
  "devDependencies": {
    "@adonisjs/config": "^3.0.9",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/lucid": "^18.3.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/require-ts": "^2.0.13",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.55.0",
    "del-cli": "^5.0.0",
    "eslint": "^7.31.0",
    "mrm": "^4.1.14",
    "mysql2": "^3.1.0",
    "pg": "^8.9.0",
    "typescript": "^4.9.5",
    "git-cz": "^4.9.0",
    "lint": "^0.8.19"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [],
    "minNodeVersion": "16.13.1",
    "probotApps": []
  },
  "files": [
    "build/src",
    "build/commands",
    "build/index.d.ts",
    "build/index.js"
  ],
  "adonisjs": {
    "commands": [
      "adonis-sql-to-migration/build/commands"
    ]
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  }
}
